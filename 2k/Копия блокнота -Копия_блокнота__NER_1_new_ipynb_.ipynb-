"EVAL_IOU_THRESHOLD = 0.5  # для IoU‑F1 зачёт совпадения по спану\n","\n","try:\n","    import pandas as pd, regex as re, json, os, math, random\n","except Exception as e:\n","    raise\n","\n","print('Flags:', dict(USE_GLINER=USE_GLINER, USE_SPANMARKER=USE_SPANMARKER, USE_SPACY_LLM=USE_SPACY_LLM))\n","print('Note: установка внешних библиотек закомментирована — выполните при необходимости в своей среде.')\n","\n","# Установка зависимостей (РАЗКОММЕНТИРУЙТЕ при исполнении в своей среде с интернетом)\n","%pip install -q gliner span-marker transformers datasets seqeval evaluate spacy spacy-llm\n"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"iA83iWvwyO6E","outputId":"f4825297-4d51-4f18-fdec-6888b7b2d219","executionInfo":{"status":"ok","timestamp":1760972085797,"user_tz":-300,"elapsed":7,"user":{"displayName":"starBURST","userId":"16069034674890314379"}}},"outputs":[{"output_type":"stream","name":"stdout","text":["Loaded test_items: 5 | train_items: 0\n"]}],"source":["# [NEW 2025] Data prep: ожидаем JSONL с полями {text, spans:[{start,end,label}]}\n","import json, pathlib\n","from typing import List, Dict, Tuple\n","\n","DATA_JSONL = os.environ.get('NER_LAB_TEST_JSONL', '')  # путь к тестовому jsonl (валидация/тест)\n","TRAIN_JSONL = os.environ.get('NER_LAB_TRAIN_JSONL', '')  # путь к train jsonl (для SpanMarker)\n","\n","def load_jsonl(path: str):\n","    items = []\n","    if path and os.path.exists(path):\n","        with open(path, 'r', encoding='utf-8') as f:\n","            for line in f:\n","                line = line.strip()\n","                if not line:\n","                    continue\n","                items.append(json.loads(line))\n","    return items\n","\n","test_items = load_jsonl(DATA_JSONL)\n","train_items = load_jsonl(TRAIN_JSONL)\n","\n","if not test_items:\n","    # fallback sample (5 строк) для локальной проверки\n","    test_items = [\n","        {\"text\": \"Оклад 120–150 тыс. ₽ на руки, квартальные бонусы и ДМС.\",\n","         \"spans\": [{\"start\":0, \"end\":54, \"label\":\"COMP_TERM\"}]},\n","        {\"text\": \"Релокационный пакет, компенсация связи, страховка.\",\n","         \"spans\": [{\"start\":0, \"end\":48, \"label\":\"COMP_TERM\"}]},\n","        {\"text\": \"Зарплата от 90 000 ₽, премии по KPI, расширенный ДМС.\",\n","         \"spans\": [{\"start\":0, \"end\":55, \"label\":\"COMP_TERM\"}]},\n","        {\"text\": \"Гибкий график и вкусные плюшки — не считаем как компенсацию.\",\n","         \"spans\": []},\n","        {\"text\": \"Опционы/RSU и годовой бонус 10%, переезд в Казань.\",\n","         \"spans\": [{\"start\":0, \"end\":44, \"label\":\"COMP_TERM\"}]},\n","    ]\n","\n","print(f\"Loaded test_items: {len(test_items)} | train_items: {len(train_items)}\")\n","\n","def get_gold_spans(item):\n","    return [(s['start'], s['end'], s['label']) for s in item.get('spans', []) if s.get('label')=='COMP_TERM']\n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"qUcrgA16yO6F"},"outputs":[],"source":["# [NEW 2025] Метрики: exact‑F1, IoU‑F1, coverage\n","def f1_from_sets(pred_set, gold_set):\n","    tp = len(pred_set & gold_set)\n","    fp = len(pred_set - gold_set)\n","    fn = len(gold_set - pred_set)\n","    prec = tp/(tp+fp) if (tp+fp) else 0.0\n","    rec  = tp/(tp+fn) if (tp+fn) else 0.0\n","    f1   = 2*prec*rec/(prec+rec) if (prec+rec) else 0.0\n","    return prec, rec, f1\n","\n","def iou(span_a: tuple, span_b: tuple) -> float:\n","    a = set(range(span_a[0], span_a[1]))\n","    b = set(range(span_b[0], span_b[1]))\n","    return len(a & b)/len(a | b) if (a or b) else 0.0\n","\n","def spans_to_exact_set(spans):\n","    return set((s[0], s[1], s[2]) for s in spans)\n","\n","def spans_to_iou_set(pred_spans, gold_spans, thr=0.5):\n","    matched = set()\n","    used = set()\n","    for i,p in enumerate(pred_spans):\n","        for j,g in enumerate(gold_spans):\n","            if j in used:\n","                continue\n","            if p[2] != g[2]:\n","                continue\n","            if iou((p[0],p[1]), (g[0],g[1])) >= thr:\n","                matched.add((p[0],p[1],p[2]))\n","                used.add(j)\n","                break\n","    return matched, set(pred_spans), set(gold_spans)\n","\n","def evaluate_predictions(all_pred_spans, items, iou_thr=0.5):\n","    exact_p = exact_r = exact_f = 0.0\n","    iou_p = iou_r = iou_f = 0.0\n","    cov_count = 0\n","    n = len(items)\n","    for item, pred in zip(items, all_pred_spans):\n","        gold = get_gold_spans(item)\n","        if pred:\n","            cov_count += 1\n","        pset, gset = spans_to_exact_set(pred), spans_to_exact_set(gold)\n","        p,r,f = f1_from_sets(pset, gset)\n","        exact_p += p; exact_r += r; exact_f += f\n","        mset, pset2, gset2 = spans_to_iou_set(pred, gold, thr=iou_thr)\n","        tp = len(mset)\n","        fp = len(pset2) - tp\n","        fn = len(gset2) - tp\n","        p = tp/(tp+fp) if (tp+fp) else 0.0\n","        r = tp/(tp+fn) if (tp+fn) else 0.0\n","        f = 2*p*r/(p+r) if (p+r) else 0.0\n","        iou_p += p; iou_r += r; iou_f += f\n","    k = max(n,1)\n","    return {\n","        'exact_precision': exact_p/k, 'exact_recall': exact_r/k, 'exact_f1': exact_f/k,\n","        f'iou{int(iou_thr*100)}_precision': iou_p/k, f'iou{int(iou_thr*100)}_recall': iou_r/k, f'iou{int(iou_thr*100)}_f1': iou_f/k,\n","        'coverage': cov_count/k\n","    }\n"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":308,"referenced_widgets":["dc0b18ac9d914e0abf878ebd0e8478aa","d3eb031a1d494a8a97071ae9ac410b52","ca4b2482af654b74bdf1f4f458a1808b","df8266b3931c408287fa6e16a8af3cbe","2a0a477c9313457dbc7be39212ff1ab8","d48f5cd29b174fce86303d5f1a376f41","785743f9a9a8438588c3c85b4812e8cc","c5204666ea024e3385ad8dde69c70397","8532e054e6c54c31a46500438b796cdf","5989350092a9423c9542fc6b173e3bc0","f16a27bfc5474e709a063f9d72584e9d