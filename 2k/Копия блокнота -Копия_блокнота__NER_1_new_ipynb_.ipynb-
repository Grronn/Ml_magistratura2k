  # Автоматическая разметка из ваших резюме\n","    for idx, row in df.head(sample_size).iterrows():\n","        text = str(row['resume_text'])\n","\n","        if len(text) < 50:\n","            continue\n","\n","        spans = []\n","\n","        # Извлекаем зарплату из колонки Зарплата\n","        salary = str(row['Зарплата'])\n","        if salary != 'Не указано' and any(c.isdigit() for c in salary):\n","            # Находим позицию зарплаты в тексте\n","            salary_pos = text.find(salary)\n","            if salary_pos != -1:\n","                spans.append({\n","                    'start': salary_pos,\n","                    'end': salary_pos + len(salary),\n","                    'label': 'COMP_TERM'\n","                })\n","\n","        # Ищем другие условия в тексте резюме\n","        import re\n","\n","        # Паттерны для условий компенсации\n","        patterns = [\n","            (r'\\b(?:ДМС|страховк[аи]|медицинск[аяо])\\b', 'COMP_TERM'),\n","            (r'\\b(?:бонус|преми[яи]|опцион[ы]?|акци[ия])\\b', 'COMP_TERM'),\n","            (r'\\b(?:удаленн|дистанционн)\\w*\\s+работ\\w*', 'COMP_TERM'),\n","            (r'\\b(?:релокац|переезд)\\w*', 'COMP_TERM'),\n","            (r'\\bобучени[ея]\\s+(?:за\\s+счет|корпоративн)\\w*', 'COMP_TERM'),\n","        ]\n","\n","        for pattern, label in patterns:\n","            matches = re.finditer(pattern, text, re.IGNORECASE)\n","            for match in matches:\n","                spans.append({\n","                    'start': match.start(),\n","                    'end': match.end(),\n","                    'label': label\n","                })\n","\n","        if spans:\n","            test_items.append({\n","                'text': text,\n","                'spans': spans\n","            })\n","\n","    return test_items\n","\n","# Создаем тестовые данные\n","test_items = create_test_data_from_resumes(df, 100)\n","print(f\"✓ Создано {len(test_items)} тестовых примеров\")\n","\n","# 2. Вспомогательные функции для оценки\n","def get_gold_spans(item):\n","    \"\"\"Получение золотых стандартов из тестовых данных\"\"\"\n","    return [(span['start'], span['end'], span['label']) for span in item['spans']]\n","\n","def iou(span1, span2):\n","    \"\"\"Вычисление Intersection over Union для спанов\"\"\"\n","    start1, end1 = span1\n","    start2, end2 = span2\n","\n","    intersection_start = max(start1, start2)\n","    intersection_end = min(end1, end2)\n","\n","    if intersection_start >= intersection_end:\n","        return 0.0\n","\n","    intersection = intersection_end - intersection_start\n","    union = (end1 - start1) + (end2 - start2) - intersection\n","\n","    return intersection / union\n","\n","def evaluate_predictions(preds, items, iou_thr=0.5):\n","    \"\"\"Оценка предсказаний\"\"\"\n","    tp_exact, fp_exact, fn_exact = 0, 0, 0\n","    tp_iou, fp_iou, fn_iou = 0, 0, 0\n","\n","    for item, pred_spans in zip(items, preds):\n","        gold_spans = get_gold_spans(item)\n","        gold_matched_exact = [False] * len(gold_spans)\n","        gold_matched_iou = [False] * len(gold_spans)\n","\n","        # Проверяем точные совпадения\n","        for p_span in pred_spans:\n","            matched = False\n","            for i, g_span in enumerate(gold_spans):\n","                if p_span[0] == g_span[0] and p_span[1] == g_span[1] and p_span[2] == g_span[2]:\n","                    tp_exact += 1\n","                    gold_matched_exact[i] = True\n","                    matched = True\n","                    break\n","\n","            if not matched:\n","                fp_exact += 1\n","\n","        # Проверяем совпадения по IoU\n","        for p_span in pred_spans:\n","            matched = False\n","            for i, g_span in enumerate(gold_spans):\n","                if iou((p_span[0], p_span[1]), (g_span[0], g_span[1])) >= iou_thr and p_span[2] == g_span[2]:\n","                    tp_iou += 1\n","                    gold_matched_iou[i] = True\n","                    matched = True\n","                    break\n","\n","            if not matched:\n","                fp_iou += 1\n","\n","        fn_exact += sum(not matched for matched in gold_matched_exact)\n","        fn_iou += sum(not matched for matched in gold_matched_iou)\n","\n","    # Вычисляем метрики\n","    precision_exact = tp_exact / (tp_exact + fp_exact) if (tp_exact + fp_exact) > 0 else 0\n","    recall_exact = tp_exact / (tp_exact + fn_exact) if (tp_exact + fn_exact) > 0 else 0\n","    f1_exact = 2 * precision_exact * recall_exact / (precision_exact + recall_exact) if (precision_exact + recall_exact) > 0 else 0\n","\n","    precision_iou = tp_iou / (tp_iou + fp_iou) if (tp_iou + fp_iou) > 0 else 0\n","    recall_iou = tp_iou / (tp_iou + fn_iou) if (tp_iou + fn_iou) > 0 else 0\n","    f1_iou = 2 * precision_iou * recall_iou / (precision_iou + recall_iou) if (precision_iou + recall_iou) > 0 else 0\n","\n","    return {\n","        'exact_precision': precision_exact,\n","        'exact_recall': recall_exact,\n","        'exact_f1': f1_exact,\n","        'iou50_precision': precision_iou,\n","        'iou50_recall': recall_iou,\n","        'iou50_f1': f1_iou,\n","        'coverage': recall_exact  # для совместимости\n","    }\n","\n","# 3. GLiNER подход\n","print(\"\\n2. GLiNER подход для извлечения условий...\")\n","\n","gliner_preds = None\n","if USE_GLINER:\n","    try:\n","        from gliner import GLiNER\n","\n","        print(\"Загрузка GLiNER модели...\")\n","        gliner = GLiNER.from_pretrained(\"urchade/gliner_multi\")\n","\n","        # Специфичные для резюме лейблы\n","        labels = [\n","            \"salary\", \"compensation\", \"bonus\", \"insurance\",\n","            \"remote work\", \"relocation\", \"training\", \"benefit\"\n","        ]\n","\n","        gliner_preds = []\n","        for item in test_items:\n","            try:\n","                text = item['text']\n","                preds = gliner.predict_entities(text, labels, threshold=0.3)\n","\n","                spans = []\n","                for pred in preds:\n","                    spans.append((pred['start'], pred['end'], 'COMP_TERM'))\n","\n","                gliner_preds.append(spans)\n","            except Exception as e:\n","                gliner_preds.append([])\n","\n","        print(f\"✓ GLiNER готов: {len(gliner_preds)} предсказаний\")\n","\n","    except Exception as e:\n","        print(f\"✗ GLiNER ошибка: {e}\")\n","        gliner_preds = None\n","else:\n","    print(\"GLiNER отключен\")\n","\n","# 4. Rule-Based подход для ваших резюме\n","print(\"\\n3. Rule-Based подход для извлечения условий...\")\n","\n","def extract_compensation_terms(text):\n","    \"\"\"Извлечение условий компенсации из текста резюме\"\"\"\n","    spans = []\n","\n","    if not text or len(text) < 10:\n","        return spans\n","\n","    import re\n","\n","    # Паттерны для зарплат\n","    salary_patterns = [\n","        r'\\d+[\\,\\d\\s]*(?:₽|руб|рублей|USD|\\$)(?:\\s*(?:на\\s+руки|чистыми|грязными))?',\n","        r'(?:зарплат[ауы]?|оклад[ау]?|оплат[ау]?)\\s*[:\\-\\s]*\\d+[\\,\\d\\s]*(?:₽|руб|USD|\\$)',\n","        r'\\$\\s*\\d+[\\,\\d\\s]*(?:\\s*в\\s+месяц)?',\n","    ]\n","\n","    for pattern in salary_patterns:\n","        matches = re.finditer(pattern, text, re.IGNORECASE)\n","        for match in matches:\n","            spans.append((match.start(), match.end(), 'COMP_TERM'))\n","\n","    # Бенефиты и условия\n","    benefit_terms = [\n","        (r'\\bДМС\\b', 'COMP_TERM'),\n","        (r'\\bстраховк\\w*', 'COMP_TERM'),\n","        (r'\\bмедицинск\\w*\\s+страховк\\w*', 'COMP_TERM'),\n","        (r'\\bбонус\\w*', 'COMP_TERM'),\n","        (r'\\bпреми\\w*', 'COMP_TERM'),\n","        (r'\\bопцион\\w*', 'COMP_TERM'),\n","        (r'\\bакци\\w*', 'COMP_TERM'),\n","        (r'\\bудаленн\\w*\\s+работ\\w*', 'COMP_TERM'),\n","        (r'\\bдистанционн\\w*\\s+работ\\w*', 'COMP_TERM'),\n","        (r'\\bрелокац\\w*', 'COMP_TERM'),\n","        (r'\\bпереезд\\w*', 'COMP_TERM'),\n","        (r'\\bобучени[ея]\\s+за\\s+счет\\s+компании', 'COMP_TERM'),\n","        (r'\\bкорпоративн\\w*\\s+обучени[ея]', 'COMP_TERM'),\n","    ]\n","\n","    for pattern, label in benefit_terms:\n","        matches = re.finditer(pattern, text, re.IGNORECASE)\n","        for match in matches:\n","            spans.append((match.start(), match.end(), label))\n","\n","    return spans\n","\n","# Применяем rule-based подход\n","rule_based_preds = []\n","for item in test_items:\n","    spans = extract_compensation_terms(item['text'])\n","    rule_based_preds.append(spans)\n","\n","print(f\"✓ Rule-Based готов: {len(rule_based_preds)} предсказаний\")\n","\n","# 5. Сравнение методов\n","print(\"\\n4. Сравнение методов извлечения...\")\n","\n","results = []\n","\n","# Оценка GLiNER\n","if gliner_preds is not None:\n","    gliner_metrics = evaluate_predictions(gliner_preds, test_items)\n","    results.append({\n","        'method': 'GLiNER',\n","        **gliner_metrics\n","    })\n","\n","# Оценка Rule-Based\n","rule_based_metrics = evaluate_predictions(rule_based_preds, test_items)\n","results.append({\n","    'method': 'Rule-Based',\n","    **rule_based_metrics\n","})\n","\n","# Выводим результаты\n","if results:\n","    print(\"\\n\" + \"=\"*60)\n","    print(\"РЕЗУЛЬТАТЫ СРАВНЕНИЯ\")\n","    print(\"=\"*60)\n","\n","    results_df = pd.DataFrame(results)\n","    display_cols = ['method', 'exact_precision', 'exact_recall', 'exact_f1', 'iou50_f1']\n","    display(results_df[display_cols])\n","\n","    # 6. Примеры извлечения\n","    print(\"\\n5. Примеры извлечения условий компенсации:\")\n","\n","    def show_extraction_examples(method_name, preds, items, num_examples=3):\n","        print(f\"\\n{method_name}:\")\n","\n","        examples_shown = 0\n","        for i, (item, pred_spans) in enumerate(zip(items, preds)):\n","            if examples_shown >= num_examples:\n","                break\n","\n","            if pred_spans:  # Показываем только если есть предсказания\n","                print(f\"\\nПример {i+1}:\")\n","                text_preview = item['text'][:150] + \"...\" if len(item['text']) > 150 else item['text']\n","                print(f\"  Текст: {text_preview}\")\n","                print(f\"  Извлечено: {[item['text'][s[0]:s[1]] for s in pred_spans]}\")\n","                examples_shown += 1\n","\n","    if gliner_preds is not None:\n","        show_extraction_examples(\"GLiNER\", gliner_preds, test_items)\n","\n","    show_extraction_examples(\"Rule-Based\", rule_based_preds, test_items)\n","\n","    # 7. Анализ по всем резюме\n","    print(\"\\n6. Анализ условий компенсации во всех резюме:\")\n","\n","    # Применяем лучший метод ко всем резюме\n","    best_method = rule_based_preds  # По умолчанию используем rule-based\n","\n","    all_compensation_terms = []\n","    for idx, row in df.iterrows():\n","        text = str(row['resume_text'])\n","        terms = extract_compensation_terms(text)\n","\n","        for start, end, label in terms:\n","            term_text = text[start:end]\n","            all_compensation_terms.append({\n","                'resume_id': idx,\n","                'desired_position': row['Желаемая должность'],\n","                'salary': row['Зарплата'],\n","                'term': term_text,\n","                'term_type': label\n","            })\n","\n","    # Создаем DataFrame с результатами\n","    compensation_df = pd.DataFrame(all_compensation_terms)\n","\n","    if len(compensation_df) > 0:\n","        print(f\"\\nИзвлечено {len(compensation_df)} условий компенсации:\")\n","\n","        # Группируем по типам условий\n","        term_stats = compensation_df['term_type'].value_counts()\n","        print(\"\\nРаспределение по типам:\")\n","        for term_type, count in term_stats.items():\n","            print(f\"  {term_type}: {count}\")\n","\n","        # Самые частые условия\n","        common_terms = compensation_df['term'].value_counts().head(10)\n","        print(\"\\nСамые частые условия компенсации:\")\n","        for term, count in common_terms.items():\n","            print(f\"  '{term}': {count}\")\n","\n","        # Показываем примеры\n","        print(\"\\nПримеры извлеченных условий:\")\n","        sample_terms = compensation_df.head(10)\n","        for _, term_row in sample_terms.iterrows():\n","            print(f\"  • {term_row['desired_position']}: {term_row['term']}\")\n","\n","    else:\n","        print(\"Не найдено условий компенсации в резюме\")\n","\n","    # 8. Визуализация\n","    print(\"\\n7. Визуализация результатов...\")\n","\n","    try:\n","        import matplotlib.pyplot as plt\n","        import seaborn as sns\n","\n","        if len(compensation_df) > 0:\n","            # График распределения типов условий\n","            plt.figure(figsize=(12, 8))\n","\n","            plt.subplot(2, 2, 1)\n","            term_stats.plot(kind='bar', color='skyblue')\n","            plt.title('Распределение типов условий компенсации')\n","            plt.xticks(rotation=45)\n","\n","            plt.subplot(2, 2, 2)\n","            common_terms.head(8).plot(kind='barh', color='lightcoral')\n","            plt.title('Самые частые условия')\n","\n","            plt.subplot(2, 2, 3)\n","            # Сравнение методов\n","            methods = [r['method'] for r in results]\n","            f1_scores = [r['exact_f1'] for r in results]\n","            plt.bar(methods, f1_scores, color=['blue', 'orange'])\n","            plt.title('Сравнение F1-score методов')\n","            plt.ylabel('F1 Score')\n","\n","            plt.tight_layout()\n","            plt.show()\n","\n","            print(\"✓ Диаграммы построены\")\n","\n","    except Exception as e:\n","        print(f\"✗ Ошибка визуализации: {e}\")\n","\n","print(\"\\n\" + \"=\"*80)\n","print(\"АНАЛИЗ УСЛОВИЙ КОМПЕНСАЦИИ ЗАВЕРШЕН\")\n","print(\"=\"*80)"],"metadata":{"id":"979uHY9-iiva","colab":{"base_uri":"https://localhost: