rce":["# [NEW 2025] SpanMarker (дообучение на 200 примерах) — учебный минимум\n","spanmarker_preds = None\n","if USE_SPANMARKER:\n","    try:\n","        from span_marker import SpanMarkerModel, Trainer\n","        from transformers import TrainingArguments\n","        from datasets import Dataset, DatasetDict\n","\n","        def to_token_tags(items):\n","            out = []\n","            for it in items:\n","                txt = it['text']\n","                toks = txt.split()\n","                tags = ['O'] * len(toks)\n","                spans = [(s['start'], s['end']) for s in it.get('spans', []) if s.get('label')=='COMP_TERM']\n","                char_idx = 0\n","                token_spans = []\n","                for t in toks:\n","                    start = txt.find(t, char_idx)\n","                    end = start + len(t)\n","                    token_spans.append((start,end))\n","                    char_idx = end\n","                for (s,e) in spans:\n","                    touched = [i for i,(ts,te) in enumerate(token_spans) if not (te<=s or ts>=e)]\n","                    if touched:\n","                        tags[touched[0]] = 'B-COMP_TERM'\n","                        for i in touched[1:]:\n","                            tags[i] = 'I-COMP_TERM'\n","                out.append({'tokens': toks, 'tags': tags})\n","            return out\n","\n","        if train_items:\n","            train_bio = to_token_tags(train_items)\n","        else:\n","            train_bio = to_token_tags(test_items[:3])\n","        test_bio = to_token_tags(test_items)\n","\n","        ds = DatasetDict({\n","            'train': Dataset.from_list(train_bio),\n","            'test':  Dataset.from_list(test_bio)\n","        })\n","\n","        model = SpanMarkerModel.from_pretrained(\n","            'bert-base-multilingual-cased',\n","            labels=['COMP_TERM'],\n","            model_max_length=256\n","        )\n","\n","        args = TrainingArguments(\n","            output_dir='spanmarker-comp',\n","            learning_rate=5e-5,\n","            per_device_train_batch_size=8,\n","            per_device_eval_batch_size=8,\n","            num_train_epochs=3,\n","            evaluation_strategy='epoch',\n","            logging_strategy='epoch',\n","            save_strategy='epoch',\n","            load_best_model_at_end=True,\n","        )\n","        trainer = Trainer(model=model, args=args, train_dataset=ds['train'], eval_dataset=ds['test'])\n","        try:\n","            trainer.train()\n","        except Exception as e:\n","            print('Training skipped/failed (demo environment?):', e)\n","\n","        spanmarker_preds = []\n","        for it in test_items:\n","            try:\n","                pred = model.predict(it['text'])\n","                spans = [(p['char_start_index'], p['char_end_index'], p['label']) for p in pred if p['label']=='COMP_TERM']\n","                spanmarker_preds.append(spans)\n","            except Exception as e:\n","                spanmarker_preds.append([])\n","        print('SpanMarker predictions ready:', len(spanmarker_preds))\n","    except Exception as e:\n","        print('SpanMarker unavailable:', e)\n","else:\n","    print('SpanMarker disabled (USE_SPANMARKER=False)')\n"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"CHxm5CiAyO6G","outputId":"e7ef2711-df76-4992-9442-be779d1ad411","executionInfo":{"status":"ok","timestamp":1760972290401,"user_tz":-300,"elapsed":27,"user":{"displayName":"starBURST","userId":"16069034674890314379"}}},"outputs":[{"output_type":"stream","name":"stdout","text":["spaCy‑LLM disabled (USE_SPACY_LLM=False)\n"]}],"source":["# [NEW 2025] spaCy‑LLM (LLM‑in‑the‑loop) — пример конфигурации и инференса\n","spacyllm_preds = None\n","if USE_SPACY_LLM:\n","    try:\n","        import spacy\n","        from spacy_llm.util import assemble\n","        cfg = \"\"\"\n","[nlp]\n","pipeline = [\"llm